  .m-hd{
    position:fixed;
    top:0;
    left:0;
    margin:0;
    width:100%;
    min-height:0;
    height:auto;
    z-index: 2;

  }
.m-hd span{
    position:relative;
    display: inline-block;

}
.m-hd .m-hd-body-wrap {
    position: relative;
    background-color: #1c2b41;
    border-bottom: 1px solid #d9d9d9;
    -webkit-transition:all 480ms;
    -moz-transition:all 480ms;
    transition: all 480ms;
    z-index: 6;

}
.m-hd .m-hd-subnav {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    z-index: 5;
    background-color: #f0f0f0;
    border-bottom: 1px solid #d9d9d9;
    -webkit-transition:all 480ms;
    -moz-transition:all 480ms;
    transition: all 480ms;

}
.m-hd .m-hd-body{
    position:relative;
    width:1200px;
    height:64px;
    margin:0 auto;

}
.m-hd .m-hd-body .m-hd-logo{
    /* margin-left: 20px; */
    /* width: 140px; */
    height: 64px;

}
.m-hd .m-hd-body .m-hd-logo .u-logo{
    position: relative;
    display: inline-block;
    color: #fff;
    /* width: 160px; */
    height: 24px;
    margin: 8px 6px 0 0;
    /*border-right: 1px solid #ccc; */
    vertical-align: top;
    /*img{
            width:auto;
            height: 130%;
            margin-top: -5px;
            border:0;
          }*/

}
.m-hd .m-hd-body .m-hd-logo .u-logo .logo {
    display: inline-block;
    /* width: 96px; */
    width: 108px;
    height: 27px;
    margin-top: 11px;
    margin-right: 21px;
    /* background-image: url(/images/logo/hd_logo_white.png); */
    background-image: url(/images/logo/logo-white@2x.png);
    background-size: cover;

}
.m-hd .m-hd-body .m-hd-logo .u-sublogo {
    margin-top: 12px;
    line-height: 36px;
    font-size: 18px;
    color: #d9d9d9

}
.m-hd .m-hd-body .m-hd-logo .u-sublogo:hover{
    color:#238efa;

}
.m-hd .m-hd-body .m-hd-nav{
    vertical-align: top;
    margin: 0;

}
.m-hd .m-hd-body .m-hd-nav .m-hd-nav-tab{
    position:relative;
    display: inline-block;
    height: auto;
    vertical-align: top;
    font-size: 14px;

}
.m-hd .m-hd-body .m-hd-nav .m-hd-nav-tab .u-tab {
    min-width: 40px;
    margin-right: 26px;
    padding: 22px 2px 6px 2px;
    line-height: 20px;
    color: #fff;
    text-align: center

}
.m-hd .m-hd-body .m-hd-nav .m-hd-nav-tab .u-tab:hover{
    /*background-color: $color_blue_3;*/
    border:none;

}
.m-hd .m-hd-body .m-hd-nav .m-hd-nav-tab .u-tab.active{
    /*background-color: $color_blue_3;*/
    border-bottom:2px solid #337beb;

}
.m-hd .m-hd-body .m-hd-nav .m-hd-nav-tab .u-tab-second{
    -webkit-transition:height .4s ease;
    -moz-transition:height .4s ease;
    transition: height .4s ease;
    overflow: hidden;
    position: absolute;
    top: 48px;
    left: -19px;
    padding-top: 8px;
    height: 0;
    white-space: nowrap;

}
.m-hd .m-hd-body .m-hd-nav .m-hd-nav-tab .u-tab-second .u-tab-second-icon{
    margin-left: 31px;
    display: block;
    width: 0px;
    height: 0px;
    border-bottom: 8px solid #197afc;
    border-left: 8px solid transparent;
    border-right: 8px solid transparent;

}
.m-hd .m-hd-body .m-hd-nav .m-hd-nav-tab .u-tab-second hr.u-tab-second-icon-hr{
    margin: 0;
    padding: 0;
    width: 100%;
    height: 2PX;
    border: none;
    background: #197afc;

}
.m-hd .m-hd-body .m-hd-nav .m-hd-nav-tab .u-tab-second .u-tab-second-contbox{
    width: 240px;
    display: block;
    margin: 0;
    padding: 20px 30px;
    background-color: white;
    border-left: 1px solid #eee;
    border-right: 1px solid #eee;
    border-bottom: 1px solid #eee;

}
.m-hd .m-hd-body .m-hd-nav .m-hd-nav-tab .u-tab-second .u-tab-second-contbox .u-tab-content{
    position: relative;
    display: inline-block;
    vertical-align: top;
    width: 100px;
    margin-right: 30px;

}
.m-hd .m-hd-body .m-hd-nav .m-hd-nav-tab .u-tab-second .u-tab-second-contbox .u-tab-content p{
    color: #b2b2b2;
    border-bottom: 1px solid #d9d9d9;
    padding-bottom: 10px;

}
.m-hd .m-hd-body .m-hd-nav .m-hd-nav-tab .u-tab-second .u-tab-second-contbox .u-tab-content ul li{
    white-space: nowrap;
    margin: 15px 0;
    list-style: none;

}
.m-hd .m-hd-body .m-hd-nav .m-hd-nav-tab .u-tab-second .u-tab-second-contbox .u-tab-content ul li a{
    display: inline-block;
    font-size: 14px;
    padding-bottom: 1px;
    color: #0e1021;
    border-bottom: 1px solid #fff

}
.m-hd .m-hd-body .m-hd-nav .m-hd-nav-tab .u-tab-second .u-tab-second-contbox .u-tab-content ul li a:hover{
    color:#197afc;
    border-bottom:1px solid #197afc;

}
.m-hd .m-hd-body .m-hd-nav .m-hd-nav-tab.have-second:hover .u-tab-second{
    height:300px;

}
.m-hd .m-hd-body .m-hd-search{
    position:relative;
    // display: inline-block;
    margin-top: 18px;
    height: 28px;
    background: #fff;
    vertical-align: top;
    overflow: hidden;
    float: right;
    margin-right: 5px;

}
.m-hd .m-hd-body .m-hd-search .u-box{
    position: relative;
    display: inline-block;
    width:180px;
    border: 1px solid #ccc;
    color: #999;

}
.m-hd .m-hd-body .m-hd-search input {
    position: relative;
    width:150px;
    padding: 5px 6px 5px 5px;
    font-size: 14px;
    line-height: 1.2;
    vertical-align: top;
    border:none;
    z-index: 1;

}
.m-hd .m-hd-body .m-hd-search .u-btn{
    position: absolute;
    top: 5px;
    right: 5px;
    width: 16px;
    height: 16px;
    z-index: 1;
    background-color: #fff;
    background-image: url("/images/search/search_alt.png");
    cursor: pointer

}
.m-hd .m-hd-body .m-hd-search .u-btn:hover{
    background-image:url("/images/search/search_alt_hover.png");

}
.m-hd .m-hd-body .m-hd-search:hover{}
.m-hd .m-hd-body .m-hd-search:hover:before{
    border-top-color:#238efa;
    border-left-color:#238efa;

}
.m-hd .m-hd-body .m-hd-search:hover:after{
    border-bottom-color:#238efa;
    border-right-color:#238efa;

}
.m-hd .m-hd-body .m-hd-side{
    vertical-align: top;
    float: right;

}
.m-hd .m-hd-body .m-hd-side .u-btn{
    display: inline-block;
    margin:18px 0 18px 1px;
    padding: 5px 8px 5px 8px;
    width: 60px;
    line-height: 1.2;
    border:1px solid #fff;
    color: #fff;
    border-radius: 2px;
    text-align: center;
    font-size: 14px

}
.m-hd .m-hd-body .m-hd-side .u-btn:hover{
    color:#1c2b41;
    background-color:#fff;

}
.m-hd .m-hd-body .m-hd-side .u-btn:hover:before{
    border-top-color:#238efa;
    border-left-color:#238efa;

}
.m-hd .m-hd-body .m-hd-side .u-btn:hover:after{
    border-bottom-color:#238efa;
    border-right-color:#238efa;

}
.m-hd .m-hd-body .m-hd-side .u-btn-regist {
    background-color: #fff;
    color: #1c2b41

}
.m-hd .m-hd-body .m-hd-side .u-btn-regist:hover{
    color:#fff;
    border-color:#0d6ff4;
    background-color:#0d6ff4;

}
.m-hd .m-hd-body .m-sub-label {
    position: relative;
    display: inline-block;
    margin: 16px 0;
    padding: 0 0;
    padding-left: 26px;
    padding-right: 22px;
    width: 200px;
    height: 32px;
    line-height: 32px;
    vertical-align: top;
    /*text-align: right;*/
    border-right: 1px solid #ccc;
    color: #333;

}
.m-hd .m-hd-body .m-sub-content {
    position: relative;
    display: inline-block;
    padding: 0;
    margin: 6px 0 10px 40px;
    max-width: 800px;
    height: 40px;
    vertical-align: top;

}
.m-hd .m-hd-body .m-sub-col {
    position: relative;
    display: inline-block;
    margin: 4px;
    padding: 0;
    min-width: 250px;
    height: 18px;
    line-height: 18px;
    font-size: 14px;

}
.m-hd .m-hd-body .m-sub-col .nav-name {
    display: inline-block;
    height: inherit;
    min-width: 72px;
    margin-right: 4px;
    text-align: right;
    vertical-align: top;
    color: #999;

}
.m-hd .m-hd-body .m-sub-col .nav-cont {
    display: inline-block;
    height: inherit;
    vertical-align: top;
    color: #333;

}
.m-hd .m-hd-body .m-sub-col .nav-cont span.u-twice-line {
    display: inline-block;
    margin-left: 2px;
    margin-bottom: 2px;
    padding: 2px 8px 1px 8px;
    top: -1px;
    background-color: #fff;
    border: 1px solid #ccc;
    border-radius: 4px;
    line-height: 1.4;
    font-size: 12px;

}
.m-hd .m-hd-body .m-sub-col.u-single-line{
    height:40px;
    min-width:140px;
    line-height:38px;
    top:4px;

}
.m-hd .m-hd-body a:hover {
    color: #238efa !important;

}
.m-hd .m-hd-ctrl{
    position:absolute;
    width:500px;
    min-height: 30px;
    top:0;
    left:50%;
    margin-left:-250px;
    z-index: 4;
    -webkit-transition:all 480ms;
    -moz-transition:all 480ms;
    transition: all 480ms;

}
.m-hd .m-hd-ctrl .u-hd-ctrl{
    position:relative;
    width:100%;
    height: 50px;
    margin:-20px 0;
    overflow: hidden;
    background-color: #d9d9d9;
    border-radius:20px;
    opacity: 0.6;
    cursor: pointer;
    /*&:hover{
          margin-top:-21px;opacity:1;border:1px solid $color_gray_5;background-color: $color_white;
        }*/

}
.m-hd .m-hd-ctrl .u-hd-ctrl .u-btn{
    margin: 25px auto;
    width: 16px;
    height: 20px;
    background-image: url("/images/logo/ctrl-bar.png");

}
.m-hd.u-show .m-hd-body-wrap{
    -webkit-transform:translate(0,0) scale(1,1);
       -moz-transform:translate(0,0) scale(1,1);
        -ms-transform:translate(0,0) scale(1,1);
            transform:translate(0,0) scale(1,1);
    opacity:1;

}
.m-hd.u-show .m-hd-subnav{
    -webkit-transform:translate(0,-80px);
       -moz-transform:translate(0,-80px);
        -ms-transform:translate(0,-80px);
            transform:translate(0,-80px);

}
.m-hd.u-show .m-hd-ctrl{
    -webkit-transform:translate(0,-50px);
       -moz-transform:translate(0,-50px);
        -ms-transform:translate(0,-50px);
            transform:translate(0,-50px);

}
.m-hd.u-hide{
    height:30px;
    border-bottom:none;

}
.m-hd.u-hide .m-hd-body-wrap{
    -webkit-transform:translate(0,-80px) scale(1,1);
       -moz-transform:translate(0,-80px) scale(1,1);
        -ms-transform:translate(0,-80px) scale(1,1);
            transform:translate(0,-80px) scale(1,1);
    opacity:0;

}
.m-hd.u-hide .m-hd-subnav{
    -webkit-transform:translate(0,0);
       -moz-transform:translate(0,0);
        -ms-transform:translate(0,0);
            transform:translate(0,0);

}
.m-hd.u-hide .m-hd-ctrl{
    -webkit-transform:translate(0,0);
       -moz-transform:translate(0,0);
        -ms-transform:translate(0,0);
            transform:translate(0,0);

}